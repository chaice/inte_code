<Ehcache>

    <diskStore path="java.io.tmpdir"/>

    <!--
        maxElementsInMemory 存放元素的最大值
        eternal 缓存的常量
        timeToIdleSeconds 闲置时间
        timeToLiveSeconds 存在时间
        overflowToDisk  当内存不足时是否存入磁盘
    -->
    <defaultCache
            maxElementsInMemory="10000"
            eternal="false"
            timeToIdleSeconds="120"
            timeToLiveSeconds="120"
            overflowToDisk="true"
    />


</Ehcache>